#include<algorithm>
int getKthLargest(vector<int> &arr, int k)
{
//step 1
	vector<int> sumStore;
	int n = arr.size();
	for(int i=0;i<n;i++){
		int sum = 0;
		for(int j=i;j<n;j++){
			sum +=arr[j];
			sumStore.push_back(sum);
		}
	}
//step 2
	sort(sumStore.begin(),sumStore.end());
//step 3
	return sumStore[sumStore.size()-k];
}
